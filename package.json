{
  "name": "ctb-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:no-fix": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "docker exec -it ctb_api npm run local:test",
    "test:watch": "docker exec -it ctb_api npm run local:test:watch",
    "test:cov": "docker exec -it ctb_api npm run local:test:cov",
    "test:debug": "docker exec -it ctb_api local:test:debug",
    "local:test": "jest --config ./test/jest-e2e.json --runInBand",
    "local:test:watch": "jest --watchAll --config ./test/jest-e2e.json --runInBand ",
    "local:test:cov": "jest --watchAll --config ./test/jest-e2e.json --runInBand --coverage",
    "local:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/database/connection-export.ts ",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:run": "docker exec -it ctb_api npm run local:migrate:run",
    "migrate:revert": "docker exec -it ctb_api npm run local:migrate:revert",
    "local:migrate:run": "npm run typeorm migration:run",
    "local:migrate:revert": "npm run typeorm migration:revert",
    "seed:config": "docker exec -it ctb_api npm run local:seed:config",
    "seed:run": "docker exec -it ctb_api npm run local:seed:run:class MainSeeder",
    "seed:run:class": "docker exec -it ctb_api npm run local:seed:run:class",
    "local:seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "local:seed:run": "npm run local:seed:run:class MainSeeder",
    "local:seed:run:class": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed --configName ./src/database/connection-export.ts --s",
    "docker": "docker-compose -f docker/docker-compose.yml up -d && npm run seed:run && docker logs -f ctb_api",
    "docker:build": "docker-compose -f docker/docker-compose.yml up -V --build -d && npm run seed:run && docker logs -f ctb_api",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:down:v": "docker-compose -f docker/docker-compose.yml down --volumes"
  },
  "dependencies": {
    "@depthlabs/nestjs-state-machine": "^0.0.2",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.178",
    "@types/redis": "^2.8.29",
    "aws-sdk": "^2.1048.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.6",
    "faker": "^5.5.3",
    "handlebars": "^4.7.7",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "nanoid": "^3.1.30",
    "nestjs-i18n": "^8.2.2",
    "nestjs-typeorm-paginate": "^3.1.3",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "redis": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.7",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "27.0.6",
    "nodemailer-mock": "^1.5.11",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
